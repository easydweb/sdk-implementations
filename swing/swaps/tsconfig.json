{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "typescript-config/nextjs.json",
  "compilerOptions": {
      "rootDir": ".",
      "baseUrl": "./src",
      "paths": {
          "@/*": ["components/*"],
      },
      "plugins": [
          {
              "name": "next",
          },
      ],
  },
  "include": ["next-env.d.ts", "src", ".next/types/**/*.ts"],
  "exclude": ["node_modules"],
}